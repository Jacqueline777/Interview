pipeline {
 agent any 
 stages{
    stage('Clone Repo')
    {
     steps{echo "clone"}}
     stage ('Clean Maven')
    {
     steps{bat mvn clean
           echo "clean maven"}}
    stage('Run tests')
    {
     steps{bat mvn test
           echo "run tests"}}
    stage('Run sonarqube report')
  { steps{ echo "run report"}}
    }
}
  
